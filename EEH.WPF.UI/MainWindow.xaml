<mui:ModernWindow x:Class="EEH.WPF.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:local="clr-namespace:EEH.WPF.UI"
        mc:Ignorable="d" 
                  ContentSource="/Keyword/KeywordSearch.xaml"
        Title="MainWindow" Height="450" Width="800">
    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="KEYWORD">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="SEARCH" Source="/Keyword/KeywordSearch.xaml"></mui:Link>
                <mui:Link DisplayName="API INFO" Source="/Keyword/Settings.xaml"></mui:Link>
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
    <Grid>

    </Grid>
</mui:ModernWindow>
