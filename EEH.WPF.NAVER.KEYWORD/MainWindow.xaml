<fmui:ModernWindow x:Class="EEH.WPF.NAVER.KEYWORD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:fmui="http://firstfloorsoftware.com/ModernUI"
        xmlns:local="clr-namespace:EEH.WPF.NAVER.KEYWORD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition> </RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label x:Name="lbl" Grid.Row="0" ></Label>
        <Button Grid.Row="1" Click="Button_Click"></Button>
    </Grid>
</fmui:ModernWindow>
